$TTL	3600
@				IN		SOA		ddns.queenbee.lan. root.queenbee.lan. (
	2007010401	; Serial
	3600		; Refresh [1h]
	600			; Retry   [10m]
	86400		; Expire  [1d]
	600			; Negative Cache TTL [1h]
);

@				IN		NS		ddns.queenbee.lan.
@				IN		MX		2 ddns.queenbee.lan.

ddns			IN		A		192.168.33.2

pop				IN		CNAME	ddns
www				IN		CNAME	ddns
mail			IN		CNAME	ddns
